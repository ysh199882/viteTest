#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6/node_modules/vite/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/dist/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules/rollup/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/rollup@2.70.2/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6/node_modules/vite/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rollup@2.70.2/node_modules/rollup/dist/bin/rollup" "$@"
else
  exec node  "$basedir/../../../../../rollup@2.70.2/node_modules/rollup/dist/bin/rollup" "$@"
fi
