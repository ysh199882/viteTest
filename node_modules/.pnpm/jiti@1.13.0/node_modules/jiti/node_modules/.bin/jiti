#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules/jiti/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/jiti@1.13.0/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jiti.js" "$@"
else
  exec node  "$basedir/../../bin/jiti.js" "$@"
fi
