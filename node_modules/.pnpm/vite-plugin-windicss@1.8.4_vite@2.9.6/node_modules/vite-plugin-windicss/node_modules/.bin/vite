#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules/vite/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules/vite/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite-plugin-windicss@1.8.4_vite@2.9.6/node_modules/vite-plugin-windicss/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite-plugin-windicss@1.8.4_vite@2.9.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules/vite/bin/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules/vite/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite@2.9.6_sass@1.51.0/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules:/Users/yangshihao/Documents/Test/node_modules:/Users/yangshihao/Documents/node_modules:/Users/yangshihao/node_modules:/Users/node_modules:/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite-plugin-windicss@1.8.4_vite@2.9.6/node_modules/vite-plugin-windicss/node_modules:/Users/yangshihao/Documents/Test/vite-test/node_modules/.pnpm/vite-plugin-windicss@1.8.4_vite@2.9.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite@2.9.6_sass@1.51.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../vite@2.9.6_sass@1.51.0/node_modules/vite/bin/vite.js" "$@"
fi
